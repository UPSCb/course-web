extends layout

block content
    .container-fluid
        h1 Next Generation Genomics 2021
        h3 BNBio and University of Helsinki, Institute of Biotechnology
        h3 Venue: Helsinki and online - October 26th-28th, 2021
        hr
        a.btn.btn-lg.btn-success(href='/', role='button') Home
        span &nbsp
        a.btn.btn-lg.btn-primary(href="/connect") Connect to instance
